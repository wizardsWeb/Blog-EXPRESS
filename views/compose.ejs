<%- include("partials/header") %>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="blogTitle">
        <br>
        <label>Post</label>
        <textarea class="form-control" name="blogBody" rows="5" cols="30"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>



